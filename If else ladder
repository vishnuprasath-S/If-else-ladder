#include <stdio.h>
int main()
{
int marks;
printf("Enter the marks for C programming : \n");
scanf("%d",& marks);
if (marks >=95)
{
printf("O Grade");
}
else if(marks>=85)
{
printf("S Grade");
}
else if(marks>=75)
{
printf("A Grade");
}
else if( marks>=65)
{
printf("B Grade");
}
else if( marks=50)
{
printf("C Grade");
}
else if(marks >= 40)
{
printf("P Grade");
}
else
{
printf("Fail");
}
return 0;
}
